{
  "name": "it-for-youth-homepage",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:staging": "VITE_APP_ENV=staging npm run build",
    "build:production": "VITE_APP_ENV=production npm run build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --fix",
    "test": "echo \"No tests yet\" && exit 0",
    "deploy:staging": "npm run build:staging && echo \"Deploy to staging\"",
    "deploy:production": "npm run build:production && echo \"Deploy to production\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "health-check": "curl -f $VITE_APP_URL/health || exit 1",
    "postbuild": "echo \"Build completed successfully\""
  },
  "dependencies": {
    "framer-motion": "^11.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.29.0"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "gh-pages": "^6.2.0",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/react-helmet-async": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.8.0",
    "vite": "^7.0.6"
  }
}
